<h1 class="page-header">Administradores</h1>
                <% if @administrador.errors.any? %>
                        <div class="alert alert-danger alert-dismissable">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>
                            <ul>
                            <%= @administrador.errors.full_messages.each do |message| %>
                                <li><%= message %></li>
                            <% end %>
                            </ul>
                        </div>
                    <% end %>
                <div class="panel panel-default">
                    <div class="panel-heading">
                       <%= titulo %>
                    </div>
                    <div class="panel-body">
                        <div class="col-lg-6">
                            <%= form_for([:backoffice,@administrador]) do |f| %>
                            <div class="form-group">
                                <label>Nome</label>
                                <%= f.text_field :name, class: "form-control", placeholder: "Digite seu nome" %>
                            </div>
                            <div class="form-group">
                                <label>Email</label>
                                <%= f.text_field :email, class: "form-control", placeholder: "Ex: teste@teste.com" %>
                            </div>
                             <div class="form-group">
                                <label>Senha</label>
                                <%= f.password_field :password, class: "form-control", placeholder: "Senha" %>
                            </div>
                             <div class="form-group">
                                <label>Confirmação de senha</label>
                                <%= f.password_field :password_confirmation, class: "form-control", placeholder: "Confirmação de senha" %>
                            </div>
                            <button type="submit" class="btn bnt-default">Salvar</button>
                            <% end %>
                        </div>
                    </div>
                       
                </div>
      